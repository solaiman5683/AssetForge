# .env.example - Copy to .env and configure

# Server Configuration
NODE_ENV=production
PORT=5173

# File Processing Limits
MAX_FILE_SIZE=50              # Maximum file size in MB
RATE_LIMIT_WINDOW=15          # Rate limit window in minutes
RATE_LIMIT_MAX=50             # Maximum requests per window per IP

# Performance
NODE_OPTIONS=--max-old-space-size=512    # Node memory limit in MB

# Logging
UI_LOG=false                  # Set to true for detailed logging

# Optional: Analytics (future)
# ANALYTICS_ID=
# SENTRY_DSN=

# Optional: Storage (future)
# S3_BUCKET=
# S3_ACCESS_KEY=
# S3_SECRET_KEY=
